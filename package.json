{
  "name": "september-countdown",
  "version": "1.0.0",
  "description": "Express server with daily push notifications counting down to September 13th",
  "main": "server.js",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "node server.js",
    "start:prod": "cross-env NODE_ENV=production node server.js",
    "dev": "nodemon server.js --watch",
    "generate-vapid": "node scripts/generate-vapid.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.19.2",
    "helmet": "^6.0.0",
    "luxon": "^3.4.0",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "web-push": "^3.6.4"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.0"
  },
  "keywords": [
    "push",
    "notifications",
    "express",
    "september13",
    "countdown"
  ],
  "author": "Your Name",
  "license": "MIT"
}
